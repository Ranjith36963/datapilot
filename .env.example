# DataPilot Environment Variables
# Copy this file to .env and fill in your values

# Required: Groq API key (get one free at https://console.groq.com)
GROQ_API_KEY=your_groq_api_key_here

# LLM provider: groq (default), ollama, claude, openai
DATAPILOT_LLM_PROVIDER=groq

# Groq settings
GROQ_MODEL=llama-3.3-70b-versatile

# Ollama settings (only if using ollama provider)
# OLLAMA_HOST=http://localhost:11434
# OLLAMA_MODEL=llama3.2

# Claude settings (only if using claude provider)
# ANTHROPIC_API_KEY=sk-ant-...

# OpenAI settings (only if using openai provider)
# OPENAI_API_KEY=sk-...

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000
